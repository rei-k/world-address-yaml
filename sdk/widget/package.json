{
  "name": "@vey/widget",
  "version": "1.0.0",
  "description": "Universal Shadow Widget for Vey World Address SDK - Framework-agnostic embeddable address forms",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "widget",
    "address",
    "world-address",
    "vey",
    "web-component",
    "shadow-dom"
  ],
  "author": "Vey Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rei-k/world-address-yaml"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vey/core": "^1.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0"
  }
}
