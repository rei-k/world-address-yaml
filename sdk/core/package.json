{
  "name": "@vey/core",
  "version": "1.0.0",
  "description": "Core SDK for World Address YAML - Universal address format handling",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "address",
    "world-address",
    "postal",
    "shipping",
    "international",
    "address-validation",
    "address-format"
  ],
  "author": "Vey Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rei-k/world-address-yaml"
  },
  "files": [
    "dist",
    "data"
  ],
  "peerDependencies": {},
  "devDependencies": {
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0"
  }
}
